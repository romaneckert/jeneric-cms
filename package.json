{
    "name": "@jeneric/cms",
    "version": "2.0.4",
    "description": "jeneric cms",
    "main": "index.js",
    "scripts": {
        "start": "node_modules/@jeneric/core/bin/install.js @jeneric/core && node .",
        "start-dev": "nodemon",
        "production": "NODE_ENV=production node ."
    },
    "author": "Roman Eckert",
    "license": "MIT",
    "repository": "https://github.com/romaneckert/jeneric-cms",
    "dependencies": {
        "@jeneric/app": "2.0.1",
        "@jeneric/core": "2.0.7",
        "mongoose": "5.5.3"
    },
    "nodemonConfig": {
        "ignore": [
            "public/*",
            "node_modules/@jeneric/app"
        ],
        "delay": "100",
        "ignoreRoot": [
            ".git"
        ],
        "ext": "js,json,pug,css,js",
        "exec": "npm run start"
    },
    "devDependencies": {
        "gulp": "4.0.1",
        "gulp-autoprefixer": "6.1.0",
        "gulp-sass": "4.0.2",
        "gulp-strip-css-comments": "2.0.0"
    }
}
